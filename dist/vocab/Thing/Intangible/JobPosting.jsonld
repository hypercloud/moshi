{
  "@context": {
    "id": "@id",
    "type": "@type",
    "graph": "@graph",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfa": "http://www.w3.org/ns/rdfa#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "schema": "http://schema.org/",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "usesVocabulary": {
      "@id": "rdfa:usesVocabulary",
      "@type": "@id"
    },
    "subClassOf": {
      "@id": "rdfs:subClassOf",
      "@type": "@id"
    },
    "subPropertyOf": {
      "@id": "rdfs:subPropertyOf",
      "@type": "@id"
    },
    "comment": {
      "@id": "rdfs:comment",
      "@language": "en"
    },
    "label": {
      "@id": "rdfs:label",
      "@language": "en"
    },
    "rangeIncludes": {
      "@id": "schema:rangeIncludes",
      "@container": "@set",
      "@type": "@id"
    }
  },
  "graph": [
    {
      "id": "schema:experienceRequirements",
      "type": "rdfs:Property",
      "rangeIncludes": [
        "schema:Text"
      ],
      "comment": "Description of skills and experience needed for the position.",
      "label": "experienceRequirements"
    },
    {
      "id": "schema:educationRequirements",
      "type": "rdfs:Property",
      "rangeIncludes": [
        "schema:Text"
      ],
      "comment": "Educational background needed for the position.",
      "label": "educationRequirements"
    },
    {
      "id": "schema:url",
      "type": "rdfs:Property",
      "rangeIncludes": [
        "schema:URL"
      ],
      "comment": "URL of the item.",
      "label": "url"
    },
    {
      "id": "schema:skills",
      "type": "rdfs:Property",
      "rangeIncludes": [
        "schema:Text"
      ],
      "comment": "Skills required to fulfill this role.",
      "label": "skills"
    },
    {
      "id": "schema:employmentType",
      "type": "rdfs:Property",
      "rangeIncludes": [
        "schema:Text"
      ],
      "comment": "Type of employment (e.g. full-time, part-time, contract, temporary, seasonal, internship).",
      "label": "employmentType"
    },
    {
      "id": "schema:jobBenefits",
      "type": "rdfs:Property",
      "rangeIncludes": [
        "schema:Text"
      ],
      "comment": "Description of benefits associated with the job. Supersedes benefits.",
      "label": "jobBenefits"
    },
    {
      "id": "schema:salaryCurrency",
      "type": "rdfs:Property",
      "rangeIncludes": [
        "schema:Text"
      ],
      "comment": "The currency (coded using ISO 4217, http://en.wikipedia.org/wiki/ISO_4217 ) used for the main salary information in this job posting or for this employee.",
      "label": "salaryCurrency"
    },
    {
      "id": "schema:baseSalary",
      "type": "rdfs:Property",
      "rangeIncludes": [
        "schema:Number",
        "schema:PriceSpecification"
      ],
      "comment": "The base salary of the job or of an employee in an EmployeeRole.",
      "label": "baseSalary"
    },
    {
      "id": "schema:additionalType",
      "type": "rdfs:Property",
      "rangeIncludes": [
        "schema:URL"
      ],
      "comment": "An additional type for the item, typically used for adding more specific types from external vocabularies in microdata syntax. This is a relationship between something and a class that the thing is in. In RDFa syntax, it is better to use the native RDFa syntax - the 'typeof' attribute - for multiple types. Schema.org tools may have only weaker understanding of extra types, in particular those defined externally.",
      "label": "additionalType"
    },
    {
      "id": "schema:industry",
      "type": "rdfs:Property",
      "rangeIncludes": [
        "schema:Text"
      ],
      "comment": "The industry associated with the job position.",
      "label": "industry"
    },
    {
      "id": "schema:potentialAction",
      "type": "rdfs:Property",
      "rangeIncludes": [
        "schema:Action"
      ],
      "comment": "Indicates a potential Action, which describes an idealized action in which this thing would play an 'object' role.",
      "label": "potentialAction"
    },
    {
      "id": "schema:title",
      "type": "rdfs:Property",
      "rangeIncludes": [
        "schema:Text"
      ],
      "comment": "The title of the job.",
      "label": "title"
    },
    {
      "id": "schema:specialCommitments",
      "type": "rdfs:Property",
      "rangeIncludes": [
        "schema:Text"
      ],
      "comment": "Any special commitments associated with this job posting. Valid entries include VeteranCommit, MilitarySpouseCommit, etc.",
      "label": "specialCommitments"
    },
    {
      "id": "schema:qualifications",
      "type": "rdfs:Property",
      "rangeIncludes": [
        "schema:Text"
      ],
      "comment": "Specific qualifications required for this role.",
      "label": "qualifications"
    },
    {
      "id": "schema:hiringOrganization",
      "type": "rdfs:Property",
      "rangeIncludes": [
        "schema:Organization"
      ],
      "comment": "Organization offering the job position.",
      "label": "hiringOrganization"
    },
    {
      "id": "schema:responsibilities",
      "type": "rdfs:Property",
      "rangeIncludes": [
        "schema:Text"
      ],
      "comment": "Responsibilities associated with this role.",
      "label": "responsibilities"
    },
    {
      "id": "schema:name",
      "type": "rdfs:Property",
      "rangeIncludes": [
        "schema:Text"
      ],
      "comment": "The name of the item.",
      "label": "name"
    },
    {
      "id": "schema:description",
      "type": "rdfs:Property",
      "rangeIncludes": [
        "schema:Text"
      ],
      "comment": "A short description of the item.",
      "label": "description"
    },
    {
      "id": "schema:jobLocation",
      "type": "rdfs:Property",
      "rangeIncludes": [
        "schema:Place"
      ],
      "comment": "A (typically single) geographic location associated with the job position.",
      "label": "jobLocation"
    },
    {
      "id": "schema:image",
      "type": "rdfs:Property",
      "rangeIncludes": [
        "schema:URL",
        "schema:ImageObject"
      ],
      "comment": "An image of the item. This can be a URL or a fully described ImageObject.",
      "label": "image"
    },
    {
      "id": "schema:incentiveCompensation",
      "type": "rdfs:Property",
      "rangeIncludes": [
        "schema:Text"
      ],
      "comment": "Description of bonus and commission compensation aspects of the job. Supersedes incentives.",
      "label": "incentiveCompensation"
    },
    {
      "id": "schema:workHours",
      "type": "rdfs:Property",
      "rangeIncludes": [
        "schema:Text"
      ],
      "comment": "The typical working hours for this job (e.g. 1st shift, night shift, 8am-5pm).",
      "label": "workHours"
    },
    {
      "id": "schema:datePosted",
      "type": "rdfs:Property",
      "rangeIncludes": [
        "schema:Date"
      ],
      "comment": "Publication date for the job posting.",
      "label": "datePosted"
    },
    {
      "id": "schema:occupationalCategory",
      "type": "rdfs:Property",
      "rangeIncludes": [
        "schema:Text"
      ],
      "comment": "Category or categories describing the job. Use BLS O*NET-SOC taxonomy: http://www.onetcenter.org/taxonomy.html. Ideally includes textual label and formal code, with the property repeated for each applicable value.",
      "label": "occupationalCategory"
    },
    {
      "id": "schema:mainEntityOfPage",
      "type": "rdfs:Property",
      "rangeIncludes": [
        "schema:CreativeWork",
        "schema:URL"
      ],
      "comment": "Indicates a page (or other CreativeWork) for which this thing is the main entity being described.\n      \n      See background notes for details.\n       Inverse property: mainEntity.",
      "label": "mainEntityOfPage"
    },
    {
      "id": "schema:JobPosting",
      "type": "rdfs:Class",
      "comment": "A listing that describes a job opening in a certain organization.",
      "subClassOf": "schema:Intangible",
      "usesVocabulary": "http://schema.org/"
    },
    {
      "id": "schema:alternateName",
      "type": "rdfs:Property",
      "rangeIncludes": [
        "schema:Text"
      ],
      "comment": "An alias for the item.",
      "label": "alternateName"
    },
    {
      "id": "schema:sameAs",
      "type": "rdfs:Property",
      "rangeIncludes": [
        "schema:URL"
      ],
      "comment": "URL of a reference Web page that unambiguously indicates the item's identity. E.g. the URL of the item's Wikipedia page, Freebase page, or official website.",
      "label": "sameAs"
    }
  ]
}