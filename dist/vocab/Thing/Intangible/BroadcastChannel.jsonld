{
  "@context": {
    "id": "@id",
    "type": "@type",
    "graph": "@graph",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfa": "http://www.w3.org/ns/rdfa#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "schema": "http://schema.org/",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "usesVocabulary": {
      "@id": "rdfa:usesVocabulary",
      "@type": "@id"
    },
    "subClassOf": {
      "@id": "rdfs:subClassOf",
      "@type": "@id"
    },
    "subPropertyOf": {
      "@id": "rdfs:subPropertyOf",
      "@type": "@id"
    },
    "comment": {
      "@id": "rdfs:comment",
      "@language": "en"
    },
    "label": {
      "@id": "rdfs:label",
      "@language": "en"
    },
    "rangeIncludes": {
      "@id": "schema:rangeIncludes",
      "@container": "@set",
      "@type": "@id"
    }
  },
  "graph": [
    {
      "id": "schema:name",
      "type": "rdfs:Property",
      "rangeIncludes": [
        "schema:Text"
      ],
      "comment": "The name of the item.",
      "label": "name"
    },
    {
      "id": "schema:url",
      "type": "rdfs:Property",
      "rangeIncludes": [
        "schema:URL"
      ],
      "comment": "URL of the item.",
      "label": "url"
    },
    {
      "id": "schema:broadcastChannelId",
      "type": "rdfs:Property",
      "rangeIncludes": [
        "schema:Text"
      ],
      "comment": "The unique address by which the BroadcastService can be identified in a provider lineup. In US, this is typically a number.",
      "label": "broadcastChannelId"
    },
    {
      "id": "schema:providesBroadcastService",
      "type": "rdfs:Property",
      "rangeIncludes": [
        "schema:BroadcastService"
      ],
      "comment": "The BroadcastService offered on this channel.",
      "label": "providesBroadcastService"
    },
    {
      "id": "schema:description",
      "type": "rdfs:Property",
      "rangeIncludes": [
        "schema:Text"
      ],
      "comment": "A short description of the item.",
      "label": "description"
    },
    {
      "id": "schema:BroadcastChannel",
      "type": "rdfs:Class",
      "comment": "A unique instance of a BroadcastService on a CableOrSatelliteService lineup.",
      "subClassOf": "schema:Intangible",
      "usesVocabulary": "http://schema.org/"
    },
    {
      "id": "schema:potentialAction",
      "type": "rdfs:Property",
      "rangeIncludes": [
        "schema:Action"
      ],
      "comment": "Indicates a potential Action, which describes an idealized action in which this thing would play an 'object' role.",
      "label": "potentialAction"
    },
    {
      "id": "schema:mainEntityOfPage",
      "type": "rdfs:Property",
      "rangeIncludes": [
        "schema:URL",
        "schema:CreativeWork"
      ],
      "comment": "Indicates a page (or other CreativeWork) for which this thing is the main entity being described.\n      \n      See background notes for details.\n       Inverse property: mainEntity.",
      "label": "mainEntityOfPage"
    },
    {
      "id": "schema:additionalType",
      "type": "rdfs:Property",
      "rangeIncludes": [
        "schema:URL"
      ],
      "comment": "An additional type for the item, typically used for adding more specific types from external vocabularies in microdata syntax. This is a relationship between something and a class that the thing is in. In RDFa syntax, it is better to use the native RDFa syntax - the 'typeof' attribute - for multiple types. Schema.org tools may have only weaker understanding of extra types, in particular those defined externally.",
      "label": "additionalType"
    },
    {
      "id": "schema:image",
      "type": "rdfs:Property",
      "rangeIncludes": [
        "schema:ImageObject",
        "schema:URL"
      ],
      "comment": "An image of the item. This can be a URL or a fully described ImageObject.",
      "label": "image"
    },
    {
      "id": "schema:alternateName",
      "type": "rdfs:Property",
      "rangeIncludes": [
        "schema:Text"
      ],
      "comment": "An alias for the item.",
      "label": "alternateName"
    },
    {
      "id": "schema:broadcastServiceTier",
      "type": "rdfs:Property",
      "rangeIncludes": [
        "schema:Text"
      ],
      "comment": "The type of service required to have access to the channel (e.g. Standard or Premium).",
      "label": "broadcastServiceTier"
    },
    {
      "id": "schema:sameAs",
      "type": "rdfs:Property",
      "rangeIncludes": [
        "schema:URL"
      ],
      "comment": "URL of a reference Web page that unambiguously indicates the item's identity. E.g. the URL of the item's Wikipedia page, Freebase page, or official website.",
      "label": "sameAs"
    },
    {
      "id": "schema:inBroadcastLineup",
      "type": "rdfs:Property",
      "rangeIncludes": [
        "schema:CableOrSatelliteService"
      ],
      "comment": "The CableOrSatelliteService offering the channel.",
      "label": "inBroadcastLineup"
    }
  ]
}